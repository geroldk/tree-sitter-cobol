{
  "grammars": [
    {
      "name": "cobol",
      "camelcase": "COBOL",
      "scope": "source.cobol",
      "path": ".",
      "file-types": [
        "cob",
        "cbl",
        "cpy"
      ],
      "injection-regex": "^(cobol|cobol85)$"
    }
  ],
  "metadata": {
    "version": "0.0.1",
    "license": "MIT",
    "description": "COBOL grammar for tree-sitter",
    "authors": [
      {
        "name": "Yutaro Sakamoto"
      }
    ],
    "links": {
      "repository": "https://github.com/yutaro-sakamoto/tree-sitter-cobol"
    }
  },
  "bindings": {
    "c": true,
    "node": true
  }
}
